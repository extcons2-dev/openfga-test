# OpenFGA HTTP API base URL
# Quick local run: docker run -p 8080:8080 -p 8081:8081 -p 3000:3000 openfga/openfga run
FGA_API_URL=http://localhost:8080

# If your OpenFGA is configured with authn, set Bearer token here (optional)
FGA_API_TOKEN=

# If empty, scripts will create a new store. If set, scripts will reuse it.
FGA_STORE_ID=
FGA_STORE_NAME=crm-odontoiatrico-demo

# Model file to upload (JSON syntax accepted by the API)
FGA_MODEL_FILE=./model/crm_model.json

# Demo IDs (NO PII: use internal IDs / UUIDs)
CLINIC_ID=clinicA
PATIENT_ID=pat1
APPOINTMENT_ID=appt1
CLINICAL_RECORD_ID=cr1
ADMIN_RECORD_ID=ar1
INVENTORY_ITEM_ID=item1

# Demo users
USER_OWNER=owner1
USER_DENTIST_INT=dentistInt1
USER_DENTIST_EXT=dentistExt1
USER_HYG_INT=hygInt1
USER_HYG_EXT=hygExt1
USER_ASO=aso1
USER_RECEPTION=reception1
USER_OFFICE_MANAGER=officeMgr1
USER_AGENT=agent1
USER_TECH=tech1

# Time windows (ISO8601 UTC). If left empty, scripts auto-generate windows around "now".
# Internal assignment window (care_internal)
INTERNAL_START_TIME=
INTERNAL_END_TIME=

# External appointment window (appointment.practitioner/aso)
APPT_START_TIME=
APPT_END_TIME=
